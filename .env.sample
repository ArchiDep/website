ARCHIDEP_WEB_ENDPOINT_LIVE_VIEW_SIGNING_SALT=changeme
ARCHIDEP_WEB_ENDPOINT_SECRET_KEY_BASE=changeme
ARCHIDEP_WEB_ENDPOINT_SESSION_SIGNING_SALT=changeme
