---
permalink: /archidep.json
pretty_print: true
---
{"sections":[
{%- for section in site.data.course.sections -%}
  {"title":{{ section.title | jsonify }},"slug":{{ section.slug | jsonify }},"num":
  {{- section.num | jsonify -}}
  ,"progress":
  {{- section.progress | jsonify -}}
  ,"open":{{ section.open | jsonify }}
  ,"docs":[
  {%- for item in section.items -%}
    {"title":{{ item.title | jsonify }},"num":{{ item.num | jsonify }},"course_type":
    {{- item.course_type | jsonify -}}
    ,"course_slug":{{ item.course_slug | jsonify }} ,"section":{{ item.section | jsonify }},"section_chapter":
    {{- item.section_chapter | jsonify -}}
    ,"progress":
    {{- item.progress | jsonify -}}
    ,"slides":{{ item.has_slides | jsonify }},"url":{{ item.url | jsonify }}}
    {%- unless forloop.last %},{% endunless -%}
  {%- endfor -%}
  ]}
  {%- unless forloop.last %},{% endunless -%}
{%- endfor -%}
],"cheatsheets":[
{%- for doc in site.cheatsheets -%}
  {"title":{{ doc.title | jsonify }},"sidebar_title":{{ doc.sidebar_title | jsonify }},"slug":
  {{- doc.cheatsheet | jsonify -}}
  ,"url":{{ doc.url | jsonify }}}
  {%- unless forloop.last %},{% endunless -%}
{%- endfor -%}
]}
